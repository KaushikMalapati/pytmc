import logging
logger = logging.getLogger(__name__)

import argparse
from pytmc import FullRender
#import versioneer
import pytmc

description = """Tool for generating .db files (Currently in alpha)"""

parser = argparse.ArgumentParser(
    description = description,
    formatter_class = argparse.RawTextHelpFormatter
)

parser.add_argument(
    'input_file', metavar="INPUT", type=str, help='input file'
)

parser.add_argument(
    'output_file', metavar="OUTPUT", type=str, help='input file'
)

args = parser.parse_args()




def main():
    f = FullRender(args.input_file,args.output_file)
    f.save()








if __name__ == '__main__':
    main()

